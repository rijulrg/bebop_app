<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.bebop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".nowplaying"
            android:label="Now Playing"/>
        <activity android:name=".musiclibrary"
            android:label="Library"/>
        <activity android:name=".buyFreshTunes"
            android:label="Buy Tunes"/>
        <activity android:name=".playlists"
            android:label="Playlists"/>
        <activity android:name=".Songdetails"
            android:label="Details"></activity>
    </application>

</manifest>